{
  "name": "vermilion",
  "displayName": "Vermilion Theme's",
  "description": "A beautifully crafted dark theme inspired by deep red tones (#CF0C4D) with elegant contrasts and focus-driven design.",
  "version": "4.0.0",
  "publisher": "arkikky",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "vermilion",
        "uiTheme": "vs-dark",
        "path": "./themes/vermilion-color-theme.json"
      }
    ],
    "configurationDefaults": {
      "[Vermilion Theme's]": {
        "javascript.updateImportsOnFileMove.enabled": "always",
        "typescript.updateImportsOnFileMove.enabled": "always",
        "tabnine.experimentalAutoImports": true,
        "[javascript]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode",
          "editor.formatOnSave": true
        },
        "[typescript]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode",
          "editor.formatOnSave": true
        },
        "[javascriptreact]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode",
          "editor.formatOnSave": true
        },
        "[typescriptreact]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode",
          "editor.formatOnSave": true
        },
        "[php]": {
          "editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
          "editor.formatOnSave": true
        },
        "php.validate.executablePath": "",
        "[python]": {
          "editor.defaultFormatter": "ms-python.python",
          "editor.formatOnSave": true
        },
        "[go]": {
          "editor.defaultFormatter": "golang.go",
          "editor.formatOnSave": true
        },
        "[java]": {
          "editor.defaultFormatter": "redhat.java",
          "editor.formatOnSave": true
        },
        "[html]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode",
          "editor.formatOnSave": true
        },
        "[css]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode",
          "editor.formatOnSave": true
        },
        "[tailwindcss]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode",
          "editor.formatOnSave": true
        },
        "tailwindCSS.includeLanguages": {
          "plaintext": "html",
          "javascript": "javascript"
        },
        "[json]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode",
          "editor.formatOnSave": true
        },
        "[sql]": {
          "editor.defaultFormatter": "mtxr.sqltools",
          "editor.formatOnSave": true
        },
        "window.zoomLevel": -1,
        "window.commandCenter": false,
        "window.titleBarStyle": "custom",
        "window.newWindowProfile": "Default",
        "window.newWindowDimensions": "default",
        "window.dialogStyle": "custom",
        "workbench.tree.indent": 14,
        "workbench.tree.renderIndentGuides": "none",
        "workbench.tips.enabled": true,
        "workbench.iconTheme": "material-icon-theme",
        "workbench.sideBar.location": "left",
        "workbench.activityBar.location": "default",
        "workbench.editor.showTabs": "multiple",
        "workbench.editor.editorActionsLocation": "hidden",
        "workbench.layoutControl.enabled": true,
        "explorer.confirmDelete": true,
        "explorer.compactFolders": false,
        "explorer.enableDragAndDrop": false,
        "explorer.confirmPasteNative": true,
        "explorer.decorations.colors": true,
        "editor.minimap.autohide": "mouseover",
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.accessibilitySupport": "off",
        "editor.minimap.enabled": true,
        "security.workspace.trust.untrustedFiles": "open",
        "chat.agent.enabled": false,
        "extensions.autoUpdate": false,
        "editor.tabSize": 2,
        "editor.lineNumbers": "on",
        "editor.renderWhitespace": "all",
        "editor.snippetSuggestions": "top",
        "editor.tabCompletion": "onlySnippets",
        "editor.scrollbar.horizontal": "hidden",
        "editor.scrollbar.vertical": "hidden",
        "editor.stickyScroll.enabled": false,
        "editor.dropIntoEditor.enabled": true,
        "editor.overviewRulerBorder": false,
        "editor.glyphMargin": false,
        "editor.tabFocusMode": false,
        "editor.semanticHighlighting.enabled": false,
        "editor.occurrencesHighlight": "multiFile",
        "editor.guides.indentation": true,
        "editor.selectionHighlight": true,
        "editor.quickSuggestions": {
          "other": "on",
          "comments": "on",
          "strings": "off"
        },
        "workbench.editor.enablePreview": true,
        "workbench.editor.enablePreviewFromQuickOpen": true,
        "editor.renderControlCharacters": false,
        "editor.smoothScrolling": true,
        "editor.hover.enabled": true,
        "editor.linkedEditing": true,
        "editor.bracketPairColorization.enabled": true,
        "material-icon-theme.folders.color": "#CF0C4D",
        "material-icon-theme.hidesExplorerArrows": true,
        "material-icon-theme.folders.theme": "specific",
        "editor.formatOnSave": true,
        "files.autoSave": "afterDelay",
        "files.trimTrailingWhitespace": false,
        "files.trimFinalNewlines": true,
        "files.insertFinalNewline": true,
        "explorer.confirmDragAndDrop": true,
        "prettier.enable": true,
        "files.exclude": {
          "**/.DS_Store": true,
          "**/.git": false,
          "**/node_modules": false
        },
        "explorer.autoReveal": false,
        "editor.fontFamily": "JetBrains Mono, Fira Code, monospace",
        "editor.fontSize": 12,
        "editor.lineHeight": 18,
        "editor.fontWeight": "300",
        "editor.folding": true,
        "editor.wordWrap": "on",
        "editor.formatOnPaste": true,
        "editor.fontLigatures": false,
        "editor.renderLineHighlight": "line",
        "editor.renderFinalNewline": "on",
        "editor.cursorBlinking": "smooth",
        "editor.cursorStyle": "line",
        "editor.cursorWidth": 4,
        "breadcrumbs.enabled": true,
        "breadcrumbs.icons": false,
        "breadcrumbs.filePath": "last",
        "breadcrumbs.symbolPath": "last",
        "breadcrumbs.showArrays": false,
        "breadcrumbs.showObjects": false,
        "breadcrumbs.showKeys": false,
        "terminal.integrated.defaultProfile.osx": "zsh",
        "terminal.integrated.fontSize": 12,
        "terminal.integrated.cursorStyle": "line",
        "terminal.integrated.cursorWidth": 3,
        "terminal.integrated.cursorBlinking": true,
        "terminal.integrated.enableMultiLinePasteWarning": "always",
        "terminal.integrated.scrollback": 5000,
        "terminal.integrated.env.osx": {
          "NODE_ENV": "development"
        },
        "workbench.reduceMotion": true,
        "files.simpleDialog.enable": true,
        "telemetry.telemetryLevel": "off"
      }
    }
  },
  "icon": "images/vermilion.png",
  "scripts": {
    "build": "echo 'No build step needed'",
    "release": "vsce publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arkikky/vermilion.git"
  },
  "keywords": [
    "vscode",
    "developer-tools",
    "vscode-theme",
    "vscode-configs",
    "dark-mode",
    "engineering-tools",
    "light-mode"
  ],
  "bugs": {
    "url": "https://github.com/arkikky/vermilion/issues"
  },
  "homepage": "https://github.com/arkikky/vermilion#readme",
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "ansi-regex": "^5.0.1",
    "argparse": "^1.0.10",
    "array-union": "^2.1.0",
    "better-path-resolve": "^1.0.0",
    "braces": "^3.0.3",
    "chardet": "^2.1.0",
    "ci-info": "^3.9.0",
    "cross-spawn": "^7.0.6",
    "detect-indent": "^6.1.0",
    "dir-glob": "^3.0.1",
    "enquirer": "^2.4.1",
    "esprima": "^4.0.1",
    "extendable-error": "^0.1.7",
    "fast-glob": "^3.3.3",
    "fastq": "^1.19.1",
    "fill-range": "^7.1.1",
    "find-up": "^4.1.0",
    "fs-extra": "^7.0.1",
    "glob-parent": "^5.1.2",
    "globby": "^11.1.0",
    "graceful-fs": "^4.2.11",
    "human-id": "^4.1.2",
    "iconv-lite": "^0.7.0",
    "ignore": "^5.3.2",
    "is-extglob": "^2.1.1",
    "is-glob": "^4.0.3",
    "is-number": "^7.0.0",
    "is-subdir": "^1.2.0",
    "is-windows": "^1.0.2",
    "isexe": "^2.0.0",
    "js-yaml": "^3.14.1",
    "jsonfile": "^4.0.0",
    "locate-path": "^5.0.0",
    "lodash.startcase": "^4.4.0",
    "merge2": "^1.4.1",
    "micromatch": "^4.0.8",
    "mri": "^1.2.0",
    "outdent": "^0.5.0",
    "p-filter": "^2.1.0",
    "p-limit": "^2.3.0",
    "p-locate": "^4.1.0",
    "p-map": "^2.1.0",
    "p-try": "^2.2.0",
    "package-manager-detector": "^0.2.11",
    "path-exists": "^4.0.0",
    "path-key": "^3.1.1",
    "path-type": "^4.0.0",
    "picocolors": "^1.1.1",
    "picomatch": "^2.3.1",
    "pify": "^4.0.1",
    "prettier": "^2.8.8",
    "quansync": "^0.2.11",
    "queue-microtask": "^1.2.3",
    "read-yaml-file": "^1.1.0",
    "resolve-from": "^5.0.0",
    "reusify": "^1.1.0",
    "run-parallel": "^1.2.0",
    "safer-buffer": "^2.1.2",
    "semver": "^7.7.3",
    "shebang-command": "^2.0.0",
    "shebang-regex": "^3.0.0",
    "signal-exit": "^4.1.0",
    "slash": "^3.0.0",
    "spawndamnit": "^3.0.1",
    "sprintf-js": "^1.0.3",
    "strip-ansi": "^6.0.1",
    "strip-bom": "^3.0.0",
    "term-size": "^2.2.1",
    "to-regex-range": "^5.0.1",
    "universalify": "^0.1.2",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "vsce": "^2.15.0"
  },
  "author": {
    "name": "ArkIkky",
    "url": "https://github.com/arkikky/vermilion/",
    "email": "ikky.andreansyah@gmail.com"
  },
  "license": "MIT"
}
